wfpos:ResponsibilityStatementAnalysis-difSurveyFormula
        rdf:type        sh:PropertyShape ;
        sh:class        wfpos:PerformanceSupportRecommendation ;
        sh:description  "Automatically calculated based on the Development Priority value." ;
        sh:group        wfpos:PerformanceSupportReviewAndRecommendationPropertyGroup ;
        sh:name         "initial recommendation of performance support needs" ;
        sh:order        "1"^^xsd:decimal ;
        sh:path         wfpos:difSurveyFormula ;
        sh:values       [ dash:js  "let devP = focusNode.developmentPriority.valueOf(\"dtype:literal\")\r\nlet initialRecommendation = null\r\nif (devP >= -9 && devP <= 0) {\r\n                initialRecommendation = graph.namedNode(\"http://nwcg.gov/data/psat/Level1Recommendation\")\r\n                } else if (devP > 0 && devP <= 3) {\r\n             initialRecommendation = graph.namedNode(\"http://nwcg.gov/data/psat/Level2Recommendation\")\r\n} else if (devP > 3 && devP <= 4.5) {\r\n    initialRecommendation = graph.namedNode(\"http://nwcg.gov/data/psat/Level3Recommendation\")\r\n} else if (devP > 4.5 && devP <= 6) {\r\n    initialRecommendation = graph.namedNode(\"http://nwcg.gov/data/psat/Level4Recommendation\")\r\n} else if (devP > 6 && devP <= 7.5) {\r\n    initialRecommendation = graph.namedNode(\"http://nwcg.gov/data/psat/Level5Recommendation\")\r\n}\r\ninitialRecommendation" ] .

