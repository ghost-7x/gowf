<urn:x-evn-master:nwcg_org>
        rdf:type              owl:Ontology , teamwork:Vocabulary , ontologyprojects:Ontology ;
        rdfs:comment          "A model describing the make-up of the NWCG organizational structure." ;
        rdfs:label            "WF Schema - NWCG Organization" ;
        edg:subjectArea       <urn:x-tb-governance:WF> ;
        metadata:status       metadata:UnderDevelopmentStatus ;
        swa:defaultNamespace  "http://nwcg.gov/model/nwcg_org#" ;
        teamwork:rootClass    edg:Organization ;
        owl:imports           <http://edg.topbraid.solutions/1.0/schema/core> , <http://datashapes.org/graphql> , <http://topbraid.org/teamworkconstraints> .

nwcg_org:Unit  rdf:type  edg:AssetClass , sh:NodeShape ;
        rdfs:comment     "A long-standing body of subject matter experts representing a subset of an NWCG subcommittee’s functional or business area. Meets objectives stated in its mission statement.\n\nEstablished by and report to subcommittees." ;
        rdfs:label       "Unit" ;
        rdfs:subClassOf  edg:Organization ;
        edg:acronym      "UNIT" .

nwcg_org:Committee  rdf:type  edg:AssetClass , sh:NodeShape ;
        rdfs:comment     "A long-standing body of subject matter experts representing one of the primary NWCG functional or business areas. Develops and maintains NWCG standards and associated support materials. Meets objectives stated in its mission statement.  May establish a subcommittee structure as workload requires.\n\nEstablished by and report to the NWCG Executive Board." ;
        rdfs:label       "Committee" ;
        rdfs:subClassOf  edg:Organization ;
        edg:acronym      "CMTE" .

ontologyprojects:Ontology
        rdfs:subClassOf  teamwork:Vocabulary .

teamwork:Vocabulary  rdfs:subClassOf  owl:Ontology .

nwcg_org:Subcommittee
        rdf:type         edg:AssetClass , sh:NodeShape ;
        rdfs:comment     "A long-standing body of subject matter experts representing a subset of an NWCG committee’s functional or business area. Meets objectives stated in its mission statement. May establish a unit  structure as workload requires.\n\nEstablished by and report to a parent committee." ;
        rdfs:label       "Subcommitee" , "Subcommittee" ;
        rdfs:subClassOf  edg:Organization ;
        edg:acronym      "SC" .

nwcg_org:CourseSteeringCommittee
        rdf:type         edg:AssetClass , sh:NodeShape ;
        rdfs:comment     "A long-standing body of subject matter experts responsible for oversight and maintenance of a national level course, or a geographic area delivered course maintained by a single cadre. Meets objectives stated in its mission statement.\n\nEstablished by and reports to the parent committee or their designated subgroup." ;
        rdfs:label       "Course Steering Committee" ;
        rdfs:subClassOf  edg:Organization ;
        edg:acronym      "CSCMTE" .

nwcg_org:TaskTeam  rdf:type  edg:AssetClass , sh:NodeShape ;
        rdfs:comment     "A limited-term body of subject matter experts formed via a tasking memorandum to accomplish a specific, time-bounded, undertaking for the tasking entity.\n\nEstablished by and report to any NWCG established group." ;
        rdfs:label       "Task Team" ;
        rdfs:subClassOf  edg:Organization ;
        edg:acronym      "TT" .

nwcg_org:ExecutiveBoard
        rdf:type         edg:AssetClass , sh:NodeShape ;
        rdfs:label       "Executive Board" ;
        rdfs:subClassOf  edg:Organization ;
        edg:acronym      "EB" .

nwcg_org:CurriculumManagementUnit
        rdf:type         edg:AssetClass , sh:NodeShape ;
        rdfs:label       "Curriculum Management Unit" ;
        rdfs:subClassOf  edg:Organization ;
        edg:acronym      "CMU" .

nwcg_org:Board  rdf:type  edg:AssetClass , sh:NodeShape ;
        rdfs:comment     "A long-standing body of subject matter experts organized to establish performance criteria,  evaluation processes, business requirements, and operational standards in specific NWCG business areas (e.g., airtankers, incident reporting, position naming).\n\nEstablished by and report to the Executive Board, committees, or subcommittees." ;
        rdfs:label       "Board" ;
        rdfs:subClassOf  edg:Organization ;
        edg:acronym      "BD" .
