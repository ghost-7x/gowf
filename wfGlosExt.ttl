        edg:subjectArea       <urn:x-tb-governance:WF> ;
        dc:description        "<div>6/6/2022<br />Removing:&nbsp;\n<ul>\n<li class=\"LiteralViewer\"><a title=\"http://nwcg.gov/model/glossary_extension#dataEntry\">wfglos:dataEntry</a></li>\n<li class=\"LiteralViewer\"><a title=\"http://nwcg.gov/model/glossary_extension#dataExchange\">wfglos:dataExchange</a></li>\n</ul>\nAs referenced in Data Standard Term. Not deleting from EDG entirely in case we decide to reimplement at a later date.</div>"^^rdf:HTML ;
        metadata:status       metadata:UnderDevelopmentStatus ;
        swa:defaultNamespace  "http://nwcg.gov/model/glossary_extension#" ;
        teamwork:rootClass    edg:GlossaryViewpoint ;
        owl:imports           <urn:x-evn-master:business_rules> , <http://topbraid.org/teamworkconstraints> , <urn:x-evn-master:datastd> , <urn:x-evn-master:dataenum> , <http://datashapes.org/graphql> , <http://edg.topbraid.solutions/1.0/schema/glossary> .

