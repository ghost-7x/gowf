wfpos:ResponsibilityStatementAnalysis-workEffortCalculation
        rdf:type        sh:PropertyShape ;
        sh:datatype     xsd:decimal ;
        sh:description  "Hours per 1 assigned resource. A calculated value based on the performance support materials and media identified to support the positon duty statement. Each material/media type has a set number of hours associated with it for developing a product." ;
        sh:group        wfpos:MaterialsAndWorkEffortPropertyGroup ;
        sh:name         "work effort calculation" ;
        sh:order        "3"^^xsd:decimal ;
        sh:path         wfpos:workEffortCalculation ;
        sh:values       [ dash:js  "let mediaValue = 0\r\nfocusNode.media.forEach(media => mediaValue += media.mediaDevelopmentHours)\r\nlet performanceSupportMaterialValue = 0\r\nfocusNode.performanceSupportMaterials.forEach(material => performanceSupportMaterialValue += material.developmentHours)\r\nlet workEffort = (mediaValue + performanceSupportMaterialValue) * focusNode.estimatedTimeToCompletion\r\nworkEffort" ] .

