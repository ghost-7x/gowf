ontologyprojects:Ontology
        rdfs:subClassOf  teamwork:Vocabulary .

teamwork:Vocabulary  rdfs:subClassOf  owl:Ontology .

business_rules:BusinessRule
        rdf:type         owl:Class , sh:NodeShape ;
        rdfs:comment     "Business rules are a set of conditions that govern the data attribute so that it occurs in a way that is acceptable to the business (or customer). A business rule is a statement that defines or constrains some aspect of the data. There may be many business rules that pertain directly to the attribute, or to its relationship to other attributes. " ;
        rdfs:label       "Business Rule" ;
        rdfs:subClassOf  owl:Thing ;
        sh:property      business_rules:BusinessRule-governsExchange , business_rules:BusinessRule-usedByTerm , edg:Identifiable-type , business_rules:BusinessRule-label , edg:Narratable-description , business_rules:BusinessRule-governsDataEntry .

business_rules:BusinessRule-usedByTerm
        rdf:type        sh:PropertyShape ;
        sh:class        wfglos:DataStandardTerm ;
        sh:description  "A relationship to the term(s) that are governed by the business rule." ;
        sh:group        edg:TraceableGroup ;
        sh:name         "used by term" ;
        sh:nodeKind     sh:IRI ;
        sh:order        "0"^^xsd:decimal ;
        sh:path         [ sh:inversePath  wfglos:businessRule ] .

<urn:x-evn-master:business_rules>
        rdf:type              owl:Ontology , teamwork:Vocabulary , ontologyprojects:Ontology ;
        rdfs:comment          "Business rules are a set of conditions that govern the data attribute so that it occurs in a way that is acceptable to the business (or customer). A business rule is a statement that defines or constrains some aspect of the data. There may be many business rules that pertain directly to the attribute, or to its relationship to other attributes. " ;
        rdfs:label            "WF Schema - Business Rules" ;
        graphql:publicClass   owl:Thing , business_rules:BusinessRule ;
        edg:subjectArea       <urn:x-tb-governance:WF> ;
        metadata:status       metadata:UnderDevelopmentStatus ;
        swa:defaultNamespace  "http://nwcg.gov/model/business_rule#" ;
        teamwork:rootClass    business_rules:BusinessRule ;
        owl:imports           <http://datashapes.org/graphql> , <http://edg.topbraid.solutions/1.0/schema/core> , <urn:x-evn-master:wfglos> , <http://topbraid.org/teamworkconstraints> .

business_rules:BusinessRule-label
        rdf:type         sh:PropertyShape ;
        dash:singleLine  true ;
        graphql:name     "labels" ;
        sh:group         edg:IdentifiersGroup ;
        sh:name          "business rule" ;
        sh:or            dash:StringOrLangString ;
        sh:order         "2"^^xsd:decimal ;
        sh:path          rdfs:label .

business_rules:BusinessRule-governsDataEntry
        rdf:type     sh:PropertyShape ;
        sh:group     edg:TraceableGroup ;
        sh:name      "governs data entry" ;
        sh:nodeKind  sh:IRI ;
        sh:order     "2"^^xsd:decimal ;
        sh:path      [ sh:inversePath  wfglos:dataEntryRule ] .

business_rules:BusinessRule-governsExchange
        rdf:type     sh:PropertyShape ;
        sh:group     edg:TraceableGroup ;
        sh:name      "governs exchange" ;
        sh:nodeKind  sh:IRI ;
        sh:order     "1"^^xsd:decimal ;
        sh:path      [ sh:inversePath  wfglos:exchangeRule ] .
